{
	"name": "TransformMovies",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MoviesDB",
						"type": "DatasetReference"
					},
					"name": "MoviesDB"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "FilterYears"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movies as string,",
				"          Title as string,",
				"          genresgenregenre as string,",
				"          YEAR as string,",
				"          Rating as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesDB",
				"MoviesDB filter(toInteger(YEAR) >= 1010 && toInteger(YEAR) <= 2000 && rlike(genresgenregenre, 'Comedy')) ~> FilterYears",
				"FilterYears sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          movies as string,",
				"          Title as string,",
				"          genresgenregenre as string,",
				"          YEAR as string,",
				"          Rating as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}